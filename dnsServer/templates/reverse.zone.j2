$TTL 86400
@   IN  SOA     {{ DNS_SERVER_INFO.name }}.{{ DOMAIN_NAME }}. root.{{ DOMAIN_NAME }} (
        2021072301  ;;Serial
        3600        ;;Refresh
        1800        ;;Retry
        604800      ;;Expire
        86400       ;;Minimum TTL
)

; Name server Information
@       IN  NS      {{ DNS_SERVER_INFO.name }}.{{ DOMAIN_NAME }}.

{{ DNS_SERVER_INFO.ip.split(".")[3] }}      IN  PTR    {{ DNS_SERVER_INFO.name }}.{{ DOMAIN_NAME }}.

{% for server in SERVERS_TO_REGISTER %}
{{ server.ip.split(".")[3] }} IN PTR {{ server.name }}.{{ DOMAIN_NAME }}.
{% endfor %}